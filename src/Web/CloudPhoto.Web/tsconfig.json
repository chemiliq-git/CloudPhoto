{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "outDir": "wwwroot/js",
    "module": "umd",
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "Scripts/**/*"
  ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
